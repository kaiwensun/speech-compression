function [ zeroCcrossCnt] = voicingDetector( frames,rate )
    %threshF = 3000;%Energy for voiced speech tends to concentrate below 3KHz.
    sgn = frames<0;
    cross = xor(sgn(1:end-1,:),sgn(2:end,:));
    crossCnt = sum(cross);
end
function [ crossCnt, ind] = voicingDetector( frames,rate )
    %threshF = 3000;%Energy for voiced speech tends to concentrate below 3KHz.
    sgn = frames<0;
    cross = xor(sgn(1:end-1,:),sgn(2:end,:));
    crossCnt = sum(cross);
    ind = 1;
end